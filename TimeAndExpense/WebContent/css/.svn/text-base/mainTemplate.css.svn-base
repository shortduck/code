/*Added to make the tooltip yellow for all the dependent pages. */
.dijitTooltipContainer {
	position: absolute;
	left: 0;
	top: -20;
	background-color: F9F7BA;
	border: 1px solid gray;
	max-width: 450px;
	font-size: smaller;
	width: 150px;
	text-align: left;
}
